<%# Link to the "Next" page
  - available local variables
    url:           url to the next page
    current_page:  a page object for the currently displayed page
    num_pages:     total number of pages
    per_page:      number of items to fetch per page
    remote:        data-remote
-%>
<span class="next">
<span <% if current_page.last? %>class="all_bt disable" <% else %> class="all_bt" <% end %>>
<span><%= link_to_unless current_page.last?, raw(t 'views.pagination.next'), url, :rel => 'next', :remote => remote %><img  src="/images/arrow_next.png" alt="users" />
</span>
</span>
</span>

